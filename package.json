{
  "name": "@ant-yasa/yasa-engine",
  "version": "1.0.1",
  "description": "yet another static analyzer",
  "main": "./src/main.js",
  "bin": "./src/main.js",
  "dependencies": {
    "@ant-yasa/uast-spec": "^0.1.21-beta.2",
    "@ant-yasa/uast-parser-java-js": "^0.1.21-beta.2",
    "@babel/core": "^7.14.6",
    "@babel/parser": "^7.16.4",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-syntax-export-default-from": "^7.16.7",
    "@babel/plugin-syntax-json-strings": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "chalk": "^2.4.2",
    "commander": "^14.0.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^7.0.1",
    "graphviz": "^0.0.9",
    "is-type-of": "^1.2.1",
    "jsonfile": "^5.0.0",
    "JSONStream": "^1.3.5",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "node-uuid": "^1.4.8",
    "semver": "^7.3.8",
    "tmp": "^0.2.3",
    "underscore": "^1.9.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "async": "^3.2.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^50.8.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "flatted": "^3.3.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-javascript-obfuscator": "^1.1.6",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-uglify-es": "^3.0.0",
    "javascript-obfuscator": "^2.19.0",
    "jscpd": "^4.0.5",
    "jsdoc": "^4.0.2",
    "mocha": "^8.4.0",
    "pkg": "^5.7.0",
    "prettier": "^3.6.2",
    "pump": "^3.0.0",
    "rimraf": "^3.0.2",
    "simple-git": "^3.27.0",
    "typescript": "^5.7.2",
    "uglify-es": "^3.3.9",
    "webpack": "^5.73.0",
    "webpack-obfuscator": "^3.4.1"
  },
  "scripts": {
    "test-js": "mocha test/javascript/test-xast-js-benchmark.js && mocha test/javascript/test-chair-benchmark.js",
    "build-uast": "gulp --cwd ./ -f gulpfile_cli.js uastDump && pkg distUastDump/uast.js",
    "build-engine": "gulp --cwd ./ -f gulpfile_cli.js engine && pkg dist/engine.js",
    "build-sym": "gulp --cwd ./ -f gulpfile_sym.js",
    "lint": "eslint .",
    "lint:duplicates": "jscpd .",
    "lint:fix": "eslint --fix ."
  },
  "install": "npm install",
  "files": [
    "dist/"
  ],
  "keywords": [
    "static analysis",
    "program verification"
  ],
  "jshintConfig": {
    "esversion": 8,
    "bitwise": true,
    "curly": false,
    "eqeqeq": true,
    "forin": false,
    "freeze": true,
    "immed": true,
    "latedef": false,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": false,
    "plusplus": false,
    "quotmark": false,
    "undef": true,
    "strict": false,
    "trailing": true,
    "unused": true,
    "withstmt": true,
    "asi": false,
    "boss": false,
    "debug": true,
    "eqnull": false,
    "evil": false,
    "expr": false,
    "funcscope": true,
    "globalstrict": true,
    "iterator": false,
    "lastsemic": true,
    "laxbreak": true,
    "laxcomma": false,
    "loopfunc": false,
    "multistr": false,
    "proto": false,
    "scripturl": false,
    "smarttabs": false,
    "shadow": false,
    "sub": true,
    "supernew": false,
    "validthis": true,
    "browser": true,
    "couch": false,
    "devel": true,
    "dojo": false,
    "jquery": true,
    "mootools": false,
    "node": true,
    "nonstandard": false,
    "phantom": true,
    "prototypejs": false,
    "rhino": false,
    "wsh": false,
    "maxerr": 100,
    "predef": [
      "require"
    ],
    "indent": 2
  },
  "pkg": {
    "scripts": [],
    "assets": [
      "src/**/*",
      "resource/**/*"
    ],
    "targets": [
      "node18-macos-arm64",
      "node18-macos-x64",
      "node18-linux-x64"
    ]
  },
  "license": "ISC",
  "repository": "git@github.com:antgroup/YASA-Engine.git"
}
